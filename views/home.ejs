        <%- include('header') %>
        <h2>Open Roles:</h2>
        <% categorias.forEach( categorias => { %>        
            
            <div>
                <h3><%- categorias.categoria %></h3>
                    <ul>
                        <% categorias.vagas.forEach( vaga => { %>                    
                            <li><a href="vaga/<%- vaga.id %>"><%- vaga.titulo %></a></li>
                        <% }) %>
                    </ul>
            </div>
        
        <% }) %>
    <%- include('footer') %>